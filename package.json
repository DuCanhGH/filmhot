{
  "name": "filmhot",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev:typecheck": "tsc -b -w --preserveWatchOutput",
    "dev:vite": "vite",
    "dev": "cross-env DEBUG=vite-plugin-pwa:* BASE_URL=/ SW_DEV=true run-p dev:*",
    "build": "tsc && cross-env DEBUG=vite-plugin-pwa:* BASE_URL=/ vite build",
    "preview": "vite preview",
    "format": "prettier --write --embedded-language-formatting=auto .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "codetest": "pnpm lint && pnpm typecheck && pnpm test",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "0.27.2",
    "cross-env": "7.0.3",
    "firebase": "9.8.4",
    "lodash-es": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-hls-player": "3.0.7",
    "react-hook-form": "7.33.1",
    "react-infinite-scroll-component": "6.1.0",
    "react-intersection-observer": "9.3.5",
    "react-lazy-load-image-component": "1.5.4",
    "react-router-dom": "6.3.0",
    "react-tuby": "0.1.23",
    "swiper": "8.3.0",
    "swr": "1.3.0",
    "tiny-invariant": "1.2.0",
    "zustand": "4.0.0-rc.1"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/plugin-syntax-flow": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.18.6",
    "@testing-library/react": "13.3.0",
    "@types/eslint": "8.4.5",
    "@types/lodash-es": "4.17.6",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-lazy-load-image-component": "1.5.2",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "@vitejs/plugin-react": "1.3.2",
    "autoprefixer": "10.4.7",
    "c8": "7.11.3",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-react": "7.30.1",
    "husky": "8.0.1",
    "jsdom": "20.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "tailwindcss": "3.1.4",
    "typescript": "4.7.4",
    "vite": "2.9.13",
    "vite-plugin-eslint": "1.6.1",
    "vite-plugin-pwa": "0.12.2",
    "vitest": "0.17.0",
    "workbox-core": "6.5.3",
    "workbox-precaching": "6.5.3",
    "workbox-routing": "6.5.3",
    "workbox-window": "6.5.3"
  }
}
